    "scheme": "TizlyNative",
    "detach": {
      "scheme": "TizlyNative"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "auto",

    "splash": {
      "image": "./assets/splash.png",
