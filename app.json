{
  "expo": {
    "name": "Tizly",
    "slug": "TizlyNative",
    "scheme": "TizlyNative",
    "detach": {
      "scheme": "TizlyNative"
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "markjvoltaire"
        }
      ]
    ],
    "version": "40",
    "orientation": "portrait",
    "icon": "./assets/newIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash2024.png",
      "resizeMode": "contain",
      "backgroundColor": "black"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.markjvoltaire.TizlyNative",
      "buildNumber": "159",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow access to photos to upload photos from your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "61c54675-42ec-4c84-86cf-09843327f703"
      }
    }
  }
}
